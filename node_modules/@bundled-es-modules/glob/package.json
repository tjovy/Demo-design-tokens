{
  "name": "@bundled-es-modules/glob",
  "version": "13.0.1",
  "description": "mirror of glob, bundled and exposed as ES module including for browser",
  "author": "Joren Broekema <joren.broekema@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "node": "./index.js",
      "types": "./index.d.ts",
      "default": "./browser.js"
    }
  },
  "scripts": {
    "prepare": "patch-package",
    "build": "node build.js",
    "test": "run-p test:*",
    "test:browser": "wds --open --node-resolve --app-index=tests/index.html",
    "test:node": "node tests/node.js",
    "test:node:custom": "node tests/node-custom.js"
  },
  "keywords": [],
  "files": [
    "index.js",
    "index.cjs",
    "browser.js",
    "*.d.ts",
    "*.d.mts"
  ],
  "license": "MIT",
  "dependencies": {
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "glob": "^13.0.1",
    "path": "^0.12.7",
    "stream": "^0.0.3",
    "string_decoder": "^1.3.0",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@bundled-es-modules/memfs": "^4.17.0",
    "@web/dev-server": "^0.4.6",
    "esbuild": "^0.25.9",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.1"
  }
}
